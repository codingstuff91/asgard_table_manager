includes:
    - vendor/larastan/larastan/extension.neon
    - phpstan-baseline.neon
parameters:
    paths:
        - app/
    level: 6
    excludePaths:
        - app/Filament/*
        - app/Http/Controllers/Auth/*
    ignoreErrors:
        - '#return type has no value type specified in iterable type array.#'
        - '#with no value type specified in iterable type array.#'

